{
    "name": "slam/php-cs-fixer-extensions",
    "description": "Slam extension of friendsofphp/php-cs-fixer",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || 8.0",
        "ext-mbstring": "*",
        "ext-tokenizer": "*",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.50",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpunit/phpunit": "^7.5.20",
        "slam/php-debug-r": "^1.6.1",
        "slam/phpstan-extensions": "^5.0.2",
        "thecodingmachine/phpstan-strict-rules": "^0.12.1"
    },
    "autoload": {
        "psr-4": {
            "SlamCsFixer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamCsFixer\\Tests\\": "tests/",
            "PhpCsFixer\\Tests\\": "vendor/friendsofphp/php-cs-fixer/tests/"
        }
    }
}
