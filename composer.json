{
    "name": "slam/php-cs-fixer-extensions",
    "description": "Slam extension of friendsofphp/php-cs-fixer",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.3.0 || ~8.4.0",
        "ext-mbstring": "*",
        "ext-tokenizer": "*",
        "friendsofphp/php-cs-fixer": "^3.67.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.1",
        "phpstan/phpstan-phpunit": "^2",
        "phpunit/phpunit": "^11.5.2"
    },
    "autoload": {
        "psr-4": {
            "SlamCsFixer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamCsFixer\\Tests\\": "tests/"
        }
    }
}
