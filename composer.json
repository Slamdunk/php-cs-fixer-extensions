{
    "name": "slam/php-cs-fixer-extensions",
    "description": "Slam extension of friendsofphp/php-cs-fixer",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-mbstring": "*",
        "ext-tokenizer": "*",
        "friendsofphp/php-cs-fixer": "^3.38.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10.41",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpunit/phpunit": "^10.4.2",
        "slam/php-debug-r": "^1.8.1",
        "slam/phpstan-extensions": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "SlamCsFixer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamCsFixer\\Tests\\": "tests/"
        }
    }
}
