{
    "name": "slam/php-cs-fixer-extensions",
    "description": "Slam extension of friendsofphp/php-cs-fixer",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpunit/phpunit": "^7.5",
        "roave/security-advisories": "dev-master",
        "slam/php-debug-r": "^1.4",
        "slam/phpstan-extensions": "^3.0",
        "thecodingmachine/phpstan-strict-rules": "^0.11"
    },
    "autoload": {
        "psr-4": {
            "SlamCsFixer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamCsFixer\\Tests\\": "tests/",
            "PhpCsFixer\\Tests\\": "vendor/friendsofphp/php-cs-fixer/tests/"
        }
    }
}
