{
    "name": "slam/php-cs-fixer-extensions",
    "description": "Slam extension of friendsofphp/php-cs-fixer",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.4.0 || ~8.5.0",
        "ext-mbstring": "*",
        "ext-tokenizer": "*",
        "friendsofphp/php-cs-fixer": "^3.90.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.32",
        "phpstan/phpstan-phpunit": "^2.0.8",
        "phpunit/phpunit": "^12.4.4"
    },
    "autoload": {
        "psr-4": {
            "SlamCsFixer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamCsFixer\\Tests\\": "tests/"
        }
    }
}
