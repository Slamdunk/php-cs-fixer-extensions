{
    "name": "slam/php-cs-fixer-extensions",
    "description": "Slam extension of friendsofphp/php-cs-fixer",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.2.0 || ~8.3.0",
        "ext-mbstring": "*",
        "ext-tokenizer": "*",
        "friendsofphp/php-cs-fixer": "^3.42.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10.50",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpunit/phpunit": "^10.5.3"
    },
    "autoload": {
        "psr-4": {
            "SlamCsFixer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamCsFixer\\Tests\\": "tests/"
        }
    }
}
